# Copy this file to .env before running the Real Dispatch demo.

# OpenClaw runtime + gateway bindings
OPENCLAW_CONFIG_DIR=./.openclaw
OPENCLAW_WORKSPACE_DIR=./.openclaw/workspace
OPENCLAW_GATEWAY_TOKEN=demo-gateway-token
OPENCLAW_GATEWAY_PORT=18789
OPENCLAW_BRIDGE_PORT=18790

# Dispatch API
OPENCLAW_IMAGE=openclaw:local
DISPATCH_API_PORT=8080
DISPATCH_API_URL=http://127.0.0.1:8080
DISPATCH_DATABASE_URL=postgres://dispatch:dispatch@127.0.0.1:5432/dispatch
DISPATCH_AUTH_ALLOW_DEV_HEADERS=true
DISPATCH_OBJECT_STORE_ENDPOINT=http://minio:9000
DISPATCH_OBJECT_STORE_BUCKET=dispatch-evidence
DISPATCH_OBJECT_STORE_KEY=minioadmin
DISPATCH_OBJECT_STORE_SECRET=minioadmin
DISPATCH_OBJECT_STORE_SCHEMES=s3,minio

# Optional demo identifiers and artifacts
DISPATCH_DEMO_ACCOUNT_ID=d3f77db0-5d1a-4f9c-b0ea-111111111111
DISPATCH_DEMO_SITE_ID=7f6a2b2c-8f1e-4f2b-b3a1-222222222222
DISPATCH_DEMO_TECH_ID=4a0f1f70-98d0-4b39-bf5f-111111111111
DISPATCH_BOOTSTRAP_EVIDENCE_PATH=./dispatch/reports/bootstrap-evidence.json
